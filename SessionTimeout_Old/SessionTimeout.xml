<?xml version="1.0" encoding="utf-8" ?>
<widget id="SessionTimeout.widget.SessionTimeout" needsEntityContext="false" xmlns="http://www.mendix.com/widget/1.0/">
    <name>SessionTimeout</name>
    <description>This widget will alert users at a specified time interval that they are about to be logged out and will redirect your users to another page, if configured.</description>

    <icon></icon>

    <properties>
        <property key="paramMinutes" type="integer" required="true" defaultValue="5">
            <caption>Timeout Clock</caption>
            <category>General</category>
            <description>Amount of time (in minutes) to consider for the session timeout.</description>
        </property>
        <property key="paramTitle" type="string" required="true" defaultValue="Session Will Timeout!">
            <caption>Alert Title</caption>
            <category>General</category>
            <description>Short title for modal alert that will display.</description>
        </property>
        <property key="paramMessage" type="string" required="true" defaultValue="It looks like you've been idle for too long. You will be logged out soon if you do not take further action.">
            <caption>Alert Message</caption>
            <category>General</category>
            <description>General message for model alert that will display.</description>
        </property>
        <property key="paramModalDuration" type="integer" required="true" defaultValue="30">
            <caption>Action Window</caption>
            <category>General</category>
            <description>Amount of time (in seconds) to wait for user action to occur before action executes.</description>
        </property>
        <property key="paramNavAwayLink" type="string" required="false">
            <caption>Redirect</caption>
            <category>General</category>
            <description>Where to send the user on logout. Users will see the login page if blank.</description>
        </property>	
    </properties>
</widget>
